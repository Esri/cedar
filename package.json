{
  "name": "arcgis-cedar",
  "version": "0.6.1",
  "description": "Visualization framework for the ArcGIS Platform",
  "main": "./dist/cedar.js",
  "scripts": {
    "lint": "jshint src || true",
    "prebuild": "npm run lint",
    "build": "rollup -c profiles/debug.js & rollup -c profiles/production.js & grunt copy:specs && grunt copy:scripts",
    "postbuild": "npm run test",
    "start": "watch 'npm run build' src & grunt",
    "test": "karma start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/esri/cedar.git"
  },
  "keywords": [
